        $(aws::route-table route-table-example-for-connection-notification-1),
        $(aws::route-table route-table-example-for-connection-notification-2),
        $(aws::route-table route-table-example-for-connection-notification-3)
    ]
end

aws::connection-notification connection-notification-example
    vpc-endpoint: $(aws::endpoint endpoint-example-for-connection-notification)
    connection-notification-arn: "arn:aws:sns:us-west-2:242040583208:gyro-instance-state"
    connection-events: [
        "Accept"
    ]
end
