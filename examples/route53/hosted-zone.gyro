aws::vpc vpc-hosted-zone-example
    cidr-block: "10.0.0.0/16"

    tags: {
        Name: "vpc-hosted-zone-example"
    }
end

aws::hosted-zone hosted-zone-example
    hosted-zone-name: "hosted-zone-example.com"
    private-zone: true
    vpcs: [
        $(aws::vpc vpc-hosted-zone-example)
    ]

    comment: "hosted-zone-example.comment"
end
